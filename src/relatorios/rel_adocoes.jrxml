		<queryString language="SQL">
			<![CDATA[SELECT ad.idadocao,p.nome,DATE_FORMAT(data_adocao, '%d/%m/%Y')as data_adocao,an.nome as nome_animal,IFNULL(d.valor,0.0) AS valor 
